{
  "name": "alpine-magic-helpers",
  "version": "0.3.3",
  "description": "A set of magic helpers to use with AlpineJS",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "build": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinbatdorf/alpine-magic-helpers.git"
  },
  "keywords": [
    "alpinejs"
  ],
  "author": "Kevin Batdorf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinbatdorf/alpine-magic-helpers/issues"
  },
  "homepage": "https://github.com/kevinbatdorf/alpine-magic-helpers#readme",
  "devDependencies": {
    "alpinejs": "^2.7.0",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "laravel-mix": "^5.0.5",
    "vue-template-compiler": "^2.6.12",
    "webpack-livereload-plugin": "^1.2.0"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
